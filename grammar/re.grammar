<autotree>

program: expression eofile
       | <error>

expression: <skip:''> alternation
          | <error>

eofile: /^\Z/

alternation: concat(s /\|/)

concat: modified_atom(s?)

modified_atom: atom modifier(?)

modifier: /[*]/

atom: char
    | '(' <commit> expression ')'
    | <error?> <reject>

char: /\\.|[^*|()]/

nil: ''
